{
    "compilerOptions": {
        "target": "ES2022",
        "module": "ES2022",
        "esModuleInterop": true,
        "emitDecoratorMetadata": true,
        "experimentalDecorators": true,
        "moduleResolution": "Node",
        "noImplicitThis": false,
        "allowJs": true,
        "strict": true,
        "skipLibCheck": true,
        "noFallthroughCasesInSwitch": true,
        "lib": [
            "ES2022",
            "dom"
        ],
    },
    "exclude": [
        "frontless.js"
    ]
}